// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Botcast {
  id          String   @id @default(uuid())
  title       String
  description String
  date        DateTime
  url         String

  @@map("botcast")
}

model Script {
  id     String @id @default(uuid())
  script Json

  @@map("script")
}

model Task {
  id     String   @id @default(uuid())
  status String
  body   String
  date   DateTime

  @@map("task")
}
